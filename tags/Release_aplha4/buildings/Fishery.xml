<?xml version="1.0" ?>



<building name="Fishery" gameID="fishery" file="workshop.png"> 
	<!-- benches block -->
	<if> 
		<!-- replace this with position index if required -->
		<BuildingOccupancy value="3" />	
		<!-- benches -->
		<include file="wsBenches.xml"/>	
	</if>
	<else>
		<include file="wsUpperBorder.xml"/>
	</else>
	<!-- end benches block -->
 
	<!-- contents -->
	
	<if>
		<PositionIndex value=2 />
		<sprite index = 171 offsetx=-2 offsety=-1/>
	</if>
	
	<if>
		<BuildingOccupancy value="3" />
		<if>
			<PositionIndex value=0 />
			<rotate>
				<sprite index = 173 />
				<sprite index = 174 />
				<sprite index = 173 />
				<sprite index = 174 />
			</rotate>
		</if>		
		<else>
			<PositionIndex value=1 />
			<rotate>
				<sprite index = 138 />
				<sprite index = 137 />
				<sprite index = 138 />
				<sprite index = 137 />
			</rotate>
		</else>
		<else>
			<PositionIndex value=7 />
			<include file="wsTub.xml" />
			<sprite index = 154 />
		</else>
	</if>		

	<include file="ws3x3Blueprint.xml"/>
	<include file="wsLowerBorder.xml"/>
	
</building> 
